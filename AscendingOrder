public class Ascending { 
public static void main(String[] args) { 
int a[]= {2,5,5,3,1); 
int b=a.length; 
int temp = 0; 
for(int i=0;i<a.length-1;i++) 
{ 
for(int j=i+1;j<a.length;j++) 
{ 
if(a[i]<a[j]) 
{ 
temp=a[i]; 
a[i]=a[j]; 
a[j]=temp; 
} 
}
} 
for(int i=0;i<a.length;i++) 
{ 
System.out.println(a[i]); 
} 
} 
} 
